{
  "name": "autoupgrade",
  "version": "1.0.6",
  "description": "Automatically upgrade dependencies subject to a passing check command or test suite.",
  "main": "src/index.ts",
  "bin": {
    "autoupgrade": "./build/index.mjs"
  },
  "scripts": {
    "start": "tsx src/index.ts",
    "build": "tsup",
    "clean": "rm -rf build",
    "prepare": "npm run clean && npm run build"
  },
  "keywords": [
    "typescript",
    "cli",
    "upgrade",
    "npm",
    "dependencies",
    "devDependencies"
  ],
  "author": "Giuseppe Burtini",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@types/progress": "^2.0.7",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "ora": "^8.2.0",
    "progress": "^2.0.3",
    "prompts": "^2.4.2"
  }
}
